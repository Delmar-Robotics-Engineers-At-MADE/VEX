in*/\nvision::signature Vision21__SIG_1 = vision::signature (1, 5079, 5957, 5518,273, 655, 464,3, 0);\nvision::signature Vision21__SIG_2 = vision::signature (2, 0, 0, 0,0, 0, 0,3, 0);\nvision Vision21 = vision (PORT21, 55, Vision21__SIG_1, Vision21__SIG_2);\n/*vex-vision-config:end*/\ncontroller Controller1 = controller(primary);\nmotor LeftDriveSmart = motor(PORT1, ratio18_1, false);\nmotor RightDriveSmart = motor(PORT10, ratio18_1, true);\ngyro DrivetrainGyro = gyro(Brain.ThreeWirePort.B);\nsmartdrive Drivetrain = smartdrive(LeftDriveSmart, RightDriveSmart, DrivetrainGyro, 319.19, 320, 40, mm, 1);\n\n\nvoid calibrateDrivetrain() {\n  wait(200, msec);\n  Brain.Screen.print(\"Calibrating\");\n  Brain.Screen.newLine();\n  Brain.Screen.print(\"Gyro\");\n  DrivetrainGyro.calibrate();\n  while (DrivetrainGyro.isCalibrating()) {\n    wait(25, msec);\n  }\n\n  // Clears the screen and returns the cursor to row 1, column 1.\n  Brain.Screen.clearScreen();\n  Brain.Screen.setCursor(1, 1);\n}\n\n\n\n// Generated code.\n\n\n\n// define variable for remote controller enable/disable\nbool RemoteControlCodeEnabled = true;\n\n\n\n// Include the V5 Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\n// User defined function\nvoid myblockfunction_FindBestVisionObject_Visualize(bool myblockfunction_FindBestVisionObject_Visualize__Visualize);\n// User defined function\nvoid myblockfunction_TurnToBestObject();\n\nint Vision21_objectIndex = 0;\n\nfloat objectCount, i, bestAspect, area, bestIndex, aspectRatio, aspectError, MaxAspectError, TargetAspect, bestAspectError, MinimumArea, MinAspectError, BaseForAngleCalc, XTolerance;\n\n// User defined function\nvoid myblockfunction_FindBestVisionObject_Visualize(bool myblockfunction_FindBestVisionObject_Visualize__Visualize) {\n  i = 1.0;\n  bestIndex = 1.0;\n  bestAspectError = 0.0;\n  bestAspect = 0.0;\n  while ((i < Vision21.objectCount + 1.0)) {\n    Vision21_objectIndex = static_cast<int>(i) - 1;\n    Brain.Screen.setFillColor(white);\n    area = Vision21.objects[Vision21_objectIndex].width * Vision21.objects[Vision21_objectIndex].height;\n    if (area > MinimumArea) {\n      Brain.Screen.setFillColor(green);\n      aspectRatio = (Vision21.objects[Vision21_objectIndex].height * 100.0) / Vision21.objects[Vision21_objectIndex].width;\n      aspectError = aspectRatio / TargetAspect;\n      if (aspectError < MaxAspectError && aspectError > MinAspectError) {\n        Brain.Screen.setFillColor(red);\n        bestIndex = i;\n        bestAspectError = aspectError;\n        bestAspect = aspectRatio;\n      }\n      else {\n      }\n    }\n    else {\n    }\n    i = i + 1.0;\n  wait(5, msec);\n  }\n  Vision21_objectIndex = static_cast<int>(bestIndex) - 1;\n  if (myblockfunction_FindBestVisionObject_Visualize__Visualize) {\n    Brain.Screen.drawRectangle(200, 50, Vision21.objects[Vision21_objectIndex].width, Vision21.objects[Vision21_objectIndex].height);\n  }\n}\n\n// User defined function\nvoid myblockfunction_TurnToBestObject() {\n  if (fabs(static_cast<float>(Vision21.objects[Vision21_objectIndex].centerX - 180.0)) > XTolerance) {\n    Drivetrain.turnToHeading((Drivetrain.heading() + atan((Vision21.objects[Vision21_objectIndex].centerX - 180.0) / BaseForAngleCalc) * 180 / M_PI), degrees, true);\n  }\n  else {\n    Drivetrain.stop();\n  }\n}\n\n// \"when started\" hat block\nint whenStarted1() {\n  Brain.Screen.setFillColor(red);\n  Drivetrain.setStopping(coast);\n  Drivetrain.setTurnVelocity(100.0, percent);\n  Drivetrain.setHeading(0.0, degrees);\n  XTolerance = 30.0;\n  BaseForAngleCalc = 160.0;\n  MinimumArea = 10000.0;\n  TargetAspect = 100.0;\n  MaxAspectError = 1.5;\n  MinAspectError = 0.7;\n  while (true) {\n    Vision21.takeSnapshot(Vision21__SIG_1);\n    if (Vision21.objectCount > 0.0) {\n      myblockfunction_FindBestVisionObject_Visualize(false);\n      myblockfunction_TurnToBestObject();\n    }\n    else {\n      Drivetrain.stop();\n    }\n  wait(5, msec);\n  }\n  return 0;\n}\n\n\nint main() {\n  // Calibrate the Drivetrain\n  calibrateDrivetrain();\n\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // wait for rotation sensor to fully initialize\n  wait(30, msec);\n\n  whenStarted1();\n}","target":"Physical"}